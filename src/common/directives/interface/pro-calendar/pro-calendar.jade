.calendar-container
  .input-group(data-ng-click='open1()')
    input(type='text' uib-datepicker-popup='{{format}}'
    data-ng-model='dt')
    label {{label}}

  input(type='text' uib-datepicker-popup='{{format}}'
  data-ng-model='dt' is-open='popup1.opened' datepicker-options='dateOptions'
  datepicker-popup-template-url="templates/calendar/popup.tpl.html"
  datepicker-template-url="templates/calendar/datepicker.tpl.html"
  data-ng-required='true' close-text='Close' alt-input-formats='altInputFormats'
  popup-placement="" class="calendar-input")


