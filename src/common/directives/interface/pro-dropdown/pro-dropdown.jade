.input-group( class="{{focus ? 'onFocus' : ''}} {{onClick ? 'onClick' : ''}}" data-ng-click="onFocus()")
   .select
        ui-select(data-ng-model="proModel" data-theme="templates/ui-select" data-on-select="select($item, $model)" data-ng-click="openBar()" data-search-enabled="searchEnable()")
            ui-select-match(placeholder="{{placeholder}}")
               span(ng-bind='$select.selected.name')
            ui-select-choices(data-repeat='item in (proItems | filter: $select.search) track by $index' )
                span(ng-bind='item.name')
   label {{label}}



