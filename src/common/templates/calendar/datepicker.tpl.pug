.uib-datepicker(ng-switch='datepickerMode', role='application', ng-keydown='keydown($event)')
  uib-daypicker(ng-switch-when='day', tabindex='0' template-url="/templates/calendar/day.html")
  uib-monthpicker(ng-switch-when='month', tabindex='0' template-url="/templates/calendar/month.html")
  uib-yearpicker(ng-switch-when='year', tabindex='0' template-url="/templates/calendar/year.html")
