a.favourite-expert(data-ng-click=":: $ctrl.goToProfile()")
  .favourite-experts-content
    .profile-preview
      .thumb.x48
        img.user-avatar(
        data-ng-class="{'is-not-uploaded': !$ctrl.consultationOwnerImage()}"
        data-ng-src="{{:: $ctrl.consultationOwnerImage() }}")

        .status-list
          span.status.availability

      .profile-name
        span {{:: $ctrl.profile.name }}
