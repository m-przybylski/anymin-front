.communicator-modals
  .expert-unavailable
    img(data-ng-src="/assets/images/search-image.png")

    .communicator-modals-content
      h2(data-translate="COMMUNICATOR.MODALS.EXPERT_UNAVAILABLE")
      p(data-translate="COMMUNICATOR.MODALS.SEND_NOTIFICATION_WHEN_AVAILABLE")

    .modal-action
      a.btn.btn-primary(data-ng-click="reject()" data-translate="COMMUNICATOR.MODALS.CANCEL")
      a.btn.btn-default(data-ng-click="accept()" data-translate="COMMUNICATOR.MODALS.SEND_NOTIFICATION")

