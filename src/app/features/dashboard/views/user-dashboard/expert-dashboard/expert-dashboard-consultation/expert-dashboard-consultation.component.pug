.expert-dashboard-consultation-row
  a.expert-dashboard-consultation-row__add-consultation(
    *ngIf="isOwnProfile && !isCompany; else alertInformation",
    (click)='add()'
  )
    span(plat-flat-fab color='primary')
      span(plat-icon iconClass='add')
    span.expert-dashboard-consultation-row__text {{'EXPERT_DASHBOARD.ADD_CONSULTATION' | translate}}

  ng-template(#alertInformation)
    .alert-information
      span(plat-icon iconClass='warning')
      p {{'EXPERT_DASHBOARD.ALERT_INFORMATION' | translate }}
        a((click)="onAlertInformationLinkClick()", [translate]="'EXPERT_DASHBOARD.ALERT_INFORMATION.LINK'")

  plat-consultation-row.expert-dashboard-consultation-row__row(*ngFor='let consultation of consultations',
    [consultation]='consultation', [expertId]="expertId", (openConsultationDetails)="onOpenConsultationDetails($event)")
