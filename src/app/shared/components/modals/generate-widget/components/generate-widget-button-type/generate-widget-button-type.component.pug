.button-types
  label.button-type(*ngFor='let button of buttons')
    input.button-types__input(type='radio', name='buttonType', [attr.value]='button.buttonType', (blur)='onBlur()',
    (change)='onChange(button.buttonType)', (focus)='onFocus(button.buttonType)', [checked]='buttonType === button.buttonType')
    svg.button-type__image(viewBox='0 0 248 248')
      use([attr.xlink:href]='buttonType === button.buttonType ? button.selected : button.notSelected')/
    .button-type__description
      .button-type__description-title {{button.title | translate}}
      .button-type__description-subtitle {{button.subtitle | translate}}
