{
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "noImplicitAny": true,
    "target": "es5",
    "noEmitOnError": false,
    "sourceMap": true,
    "moduleResolution": "node",
    "declaration": false,
    "lib": [
      "es2017",
      "dom"
    ],
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types" : [
      "angular",
      "angular-animate",
      "angular-cookies",
      "angular-dynamic-locale",
      "angular-mocks",
      "angular-permission",
      "angular-resource",
      "angular-translate",
      "angular-ui-bootstrap",
      "angular-ui-router",
      "braintree-web",
      "node",
      "croppie",
      "jasmine",
      "jquery",
      "lodash",
      "perfect-scrollbar",
      "ng-file-upload",
      "raven-js",
      "webrtc"
    ]
  }
}
