{
  "author": "Itelo Sp. z o. o.",
  "name": "anymind-platform",
  "version": "0.0.1",
  "repository": "git@git.contactis.pl:itelo/profitelo-frontend.git",
  "scripts": {
    "postinstall": "node lib/version/version.js && node lib/scripts/patch-production.js",
    "ng": "ng",
    "start": "npm run pug-watch | ng serve  --proxy-config proxy.conf.js",
    "serve": "npm run start",
    "serve:demo": "ng serve --app=demo-app --port 4201 --proxy-config proxy.conf.js",
    "serve-ssl": "npm run start -- --ssl",
    "start-mobile": "pug src/app -P --doctype html --watch | ng serve --live-reload false  --proxy-config proxy.conf.js",
    "serve-mobile": "npm run start-mobile",
    "build": "npm run postinstall && npm run pug && ng build --prod --locale pl",
    "test": "npm run pug-watch | ng test --code-coverage",
    "test-single": "npm run pug && ng test --single-run --code-coverage",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "ci-test": "npm run env && npm run postinstall && npm run lint && npm run test-single",
    "ci-deploy": "npm run env && npm run build",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "pug": "pug src -P --doctype html",
    "pug-watch": "npm run pug -- --watch",
    "env": "node lib/environment/environment.js",
    "env-staging": "npm run env staging",
    "env-production": "npm run env production"
  },
  "lint-staged": {
    "*.{ts,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts"
  },
  "private": true,
