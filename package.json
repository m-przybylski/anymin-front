{
  "author": "Itelo Sp. z o. o.",
  "name": "anymind-platform",
  "version": "0.0.1",
  "repository": "git@git.contactis.pl:itelo/profitelo-frontend.git",
  "scripts": {
    "postinstall": "node lib/version/version.js",
    "ng": "ng",
    "start": "npm run pug-watch | ng serve  --proxy-config proxy.conf.js",
    "serve": "npm run start",
    "serve:demo": "ng serve --app=demo-app --port 4201 --proxy-config proxy.conf.js",
    "serve-ssl": "npm run start -- --ssl",
    "start-mobile": "pug src/app -P --doctype html --watch | ng serve --live-reload false  --proxy-config proxy.conf.js",
    "serve-mobile": "npm run start-mobile",
    "build": "npm run postinstall && npm run pug && ng build --prod --i18n-locale pl",
    "test": "npm run pug-watch | jest --watchAll",
    "test-single": "npm run pug && jest",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "ci-test": "npm run env && npm run postinstall && npm run lint && npm run pug && jest --coverage",
    "ci-deploy": "npm run env && npm run build",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "pug": "pug src -P --doctype html",
    "pug-watch": "npm run pug -- --watch",
    "env": "node lib/environment/environment.js",
    "env-staging": "npm run env staging",
    "env-production": "npm run env production"
  },
  "lint-staged": {
    "*.{ts,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.0",
    "@angular/common": "7.2.0",
    "@angular/compiler": "7.2.0",
    "@angular/core": "7.2.0",
    "@angular/forms": "7.2.0",
    "@angular/http": "7.2.0",
    "@angular/platform-browser": "7.2.0",
    "@angular/platform-browser-dynamic": "7.2.0",
    "@angular/router": "7.2.0",
    "@anymind-ng/api": "^0.1.43",
    "@anymind-ng/core": "^0.12.4",
    "@ng-bootstrap/ng-bootstrap": "^4.0.1",
    "@ngrx/effects": "^7.0.0",
    "@ngrx/entity": "^7.0.0",
    "@ngrx/store": "^7.0.0",
    "@ngrx/store-devtools": "^7.0.0",
    "@ngx-translate/core": "^11.0.1",
    "bootstrap-sass": "^3.3.7",
    "core-js": "^2.4.1",
    "croppie": "2.5.0",
    "fs-extra": "^7.0.1",
    "libphonenumber-js": "^1.4.2",
    "machoke-sdk": "4.0.0",
    "moment": "^2.17.1",
    "ngrx-store-freeze": "^0.2.4",
    "ngx-cookie-service": "^2.0.0",
    "papaparse": "^4.6.2",
    "raven-js": "^3.10.0",
    "reset.css": "^2.0.2",
    "rxjs": "^6.3.3",
    "symbol-observable": "^1.2.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.4",
    "@angular/cli": "7.2.0",
    "@angular/compiler-cli": "7.2.0",
    "@angular/language-service": "7.2.0",
    "@types/croppie": "^2.5.3",
    "@types/es6-shim": "^0.31.32",
    "@types/google-libphonenumber": "^7.4.10",
    "@types/iban": "0.0.30",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.55",
    "@types/node": "~6.0.60",
    "@types/papaparse": "^4.1.33",
    "@types/perfect-scrollbar": "^0.6.1",
    "@types/raven-js": "0.0.19",
    "@types/webrtc": "0.0.23",
    "angular2-template-loader": "^0.6.2",
    "chalk": "^1.1.3",
    "codelyzer": "^4.0.1",
    "deceiver-core": "^1.0.0-beta.11",
    "electron": "^2.0.9",
    "fs-walk": "0.0.1",
    "git-describe": "^4.0.2",
    "husky": "^1.0.0-rc.13",
    "iban": "0.0.11",
    "jasmine-marbles": "^0.4.1",
    "jest": "^23.5.0",
    "jest-preset-angular": "^6.0.2",
    "jest-zone-patch": "0.0.8",
    "lint-staged": "^7.2.2",
    "mkdirp": "^0.5.1",
    "prettier": "^1.14.2",
    "promised-io": "^0.3.5",
    "pug": "^2.0.0-rc.4",
    "pug-cli": "^1.0.0-alpha6",
    "request-promise": "^4.1.1",
    "rxjs-tslint-rules": "^4.7.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.9.1",
    "tslint-config-prettier": "^1.14.0",
    "tslint-immutable": "^4.6.0",
    "typescript": "3.1.6",
    "wallaby-webpack": "^3.9.11"
  }
}
