    "cc": "npm run common-config",
    "ci-test": "npm run cc && npm run tr && npm run lint && npm run test-single",
    "ci-deploy": "npm run cc && npm run tr && npm run build",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "pug": "pug src --doctype html",
    "pug-watch": "pug src --doctype html --watch"
